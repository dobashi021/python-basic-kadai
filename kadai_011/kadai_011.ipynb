        "# for文で文字列を出力\n",
        "planet_list = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "for planet_list in planet_list:\n",
        "  print(planet_list)\n",
