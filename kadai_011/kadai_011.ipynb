        "\n",
        "# while文で文字列を出力\n",
        "planet_data = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "i = 0\n",
        "while i < 9:\n",
        "    print(planet_data[i])\n",
        "    i += 1\n",
        "\n"
