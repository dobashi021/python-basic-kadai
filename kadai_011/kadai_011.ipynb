        "# for文で文字列を出力\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "for array in array:\n",
        "  print(array)\n",
        "\n",
        "# while文で文字列を出力\n",
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "i = 0\n",
        "while True:\n",
        "  print(array[i])\n",
        "  if array[i] == \"冥\":\n",
        "    break\n",
        "  i += 1\n",
        "\n"
      ]
